{
  "name": "assec",
  "version": "0.7.9",
  "license": "ISC",
  "description": "ASSEC Frontend",
  "private": true,
  "workspaces": [
    "admin",
    "graph",
    "frontend"
  ],
  "author": {
    "name": "Matthieu Lassalvy",
    "email": "matthieu.lassalvy@beta.gouv.fr"
  },
  "engines": {
    "node": "^8.11.0"
  },
  "scripts": {
    "precommit": "sh ./.scripts/hooks/pre-commit",
    "dev": "nf start --env .env.foreman --port 3000",
    "mongo": "mongod --config ./mongo/mongod.dev.conf",
    "prebump": "sh ./.scripts/git-ensure-clean",
    "bump": "node ./.scripts/yarn-bump",
    "cleanup": "sh ./.scripts/yarn-cleanup",
    "postcleanup": "yarn install",
    "build": "sh ./.scripts/yarn-build",
    "compose": "docker-compose -f docker-compose.yml -p assec up --build",
    "deploy": "fab -f deploy.py deploy"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "foreman": "^2.0.0",
    "fs-extra": "^5.0.0",
    "husky": "^0.14.3",
    "prettier": "^1.11.1",
    "prettier-eslint": "^8.8.1",
    "semver": "^5.5.0"
  }
}
